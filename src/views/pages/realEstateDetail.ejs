<%
    const { contactInformation, sameAreaEstates, randomEstates, estate } = locals;
    const { contact } = estate;
    const estateAvatar = estate.avatar;
    const estatePictures = [estateAvatar, ...(estate.pictures.length ? estate.pictures : [])];
    const estateProperties = estate.properties || [];
%>
<!DOCTYPE html>
<html lang="en">
    <%- include('../partials/head', { title: "Bất động sản đang bán", css: ["real_estate_detail"] }) %>
    <body class="overflow-x-hidden">
        <%- include('../components/header', { activeIndex: 2, contactInformation }) %>
        <main class="mx-4 lg:mx-10 xl:mx-48 pt-4 lg:pt-10 pb-10">
            <div class="grid grid-cols-4 gap-10">
                <div class="col-span-4 lg:col-span-3">
                    <%- include('../components/category', {categories: [
                        {
                            name: "BĐS nghỉ dưỡng",
                            url: "/bat-dong-san-ban/bat-dong-san-nghi-duong",
                            icon: "/assets/images/icons/bat-dong-san-nghi-duong.jpg"
                        },
                        {
                            name: "Đất nền",
                            url: "/bat-dong-san-ban/dat-nen",
                            icon: "/assets/images/icons/dat-nen.jpg"
                        },
                        {
                            name: "Nhà phố",
                            url: "/bat-dong-san-ban/nha-pho",
                            icon: "/assets/images/icons/nha-pho.jpg"
                        }
                    ]}) %>
                    <div>
                        <div class="splide -mx-4 md:-mx-0 -mt-4 md:-mt-0 bg-black md:rounded-md overflow-hidden">
                            <div class="splide__track">
                                <ul class="splide__list">
                                    <% estatePictures.forEach(picture => { %>
                                        <li
                                            class="splide__slide relative"
                                            <% if (picture.type === "video") { %>
                                                data-splide-youtube="<%= picture.video %>"
                                            <% } %>
                                        >
                                            <div 
                                                class="
                                                    absolute
                                                    top-0
                                                    left-0
                                                    z-0
                                                    w-full
                                                    h-full
                                                    bg-no-repeat
                                                    bg-center
                                                    bg-cover
                                                "
                                                style="
                                                    background-image: url(<%= picture.smallThumbnail %>);
                                                    filter: blur(8px);
                                                "
                                            ></div>
                                            <div 
                                                class="
                                                    absolute
                                                    top-0
                                                    left-0
                                                    w-full
                                                    h-full
                                                "
                                                style="background-color: rgba(0, 0, 0, 0.3);"
                                            ></div>
                                            <img
                                                src="<%= picture.origin %>"
                                                alt="Hình ảnh chi tiết của <%= estate.title %>"
                                                class="absolute z-20 w-full h-full object-contain object-center"
                                                loading="lazy"
                                                decoding="async"
                                            />
                                            <% if (picture.type === "video") { %>
                                                <div
                                                    class="
                                                        play-button-container
                                                        absolute
                                                        top-0
                                                        z-20
                                                        w-full
                                                        h-full
                                                        flex
                                                        items-center
                                                        justify-center
                                                        text-red-500
                                                        cursor-pointer
                                                    "
                                                >
                                                    <div class="video-play-button">
                                                        <svg
                                                            xmlns="http://www.w3.org/2000/svg"
                                                            viewBox="0 0 24 24"
                                                            fill="currentColor"
                                                            class="w-20"
                                                        >
                                                            <path
                                                                d="M17 4H7C4 4 2 6 2 9v6c0 3 2 5 5 5h10c3 0 5-2 5-5V9c0-3-2-5-5-5zm-3.11 9.03l-2.47 1.48c-1 .6-1.82.14-1.82-1.03v-2.97c0-1.17.82-1.63 1.82-1.03l2.47 1.48c.95.58.95 1.5 0 2.07z"
                                                            ></path>
                                                        </svg>
                                                    </div>
                                                    <div class="video-loading-spinner hidden">
                                                        <div
                                                            class="
                                                                w-14
                                                                h-14
                                                                border-4
                                                                border-white
                                                                rounded-full
                                                                animate-spin
                                                            "
                                                            style="
                                                                border-top-color: transparent;
                                                            "
                                                        ></div>
                                                    </div>
                                                </div>
                                            <% } %>
                                        </li>
                                    <% }); %>
                                </ul>
                                <div
                                    class="
                                        absolute
                                        bottom-4
                                        lg:bottom-10
                                        right-4
                                        lg:right-10
                                        bg-black
                                        bg-opacity-50
                                        px-4
                                        py-1
                                        rounded-md
                                        text-white
                                        text-sm
                                        font-semibold
                                    "
                                >
                                    <span id="splideSelectedIndex">1</span>
                                    <span>/</span>
                                    <span id="totalImageAndVideo">
                                        <%= estatePictures.length %>
                                    </span>
                                </div>
                            </div>
                        </div>
                        <div id="thumbnail-slider" class="splide hidden md:block mt-2">
                            <div class="splide__track">
                                <ul class="splide__list">
                                    <% estatePictures.forEach(picture => { 
                                        if (picture.length === 0) {
                                            return;
                                        }
                                    %>
                                        <li class="splide__slide relative box-border">
                                            <img
                                                src="<%= picture.smallThumbnail %>"
                                                alt="Hình ảnh chi tiết của <%= estate.title %>"
                                                class="w-full h-full object-cover object-center"
                                                loading="lazy"
                                                decoding="async"
                                            />
                                            <% if (picture.type === "video") { %>
                                                <div
                                                    class="
                                                        absolute
                                                        top-0
                                                        left-0
                                                        right-0
                                                        bottom-0
                                                        z-10
                                                        flex
                                                        items-center
                                                        justify-center
                                                        text-white
                                                    "
                                                >
                                                    <svg
                                                        xmlns="http://www.w3.org/2000/svg"
                                                        viewBox="0 0 24 24"
                                                        fill="currentColor"
                                                        class="w-7"
                                                    >
                                                        <path
                                                            d="M17 4H7C4 4 2 6 2 9v6c0 3 2 5 5 5h10c3 0 5-2 5-5V9c0-3-2-5-5-5zm-3.11 9.03l-2.47 1.48c-1 .6-1.82.14-1.82-1.03v-2.97c0-1.17.82-1.63 1.82-1.03l2.47 1.48c.95.58.95 1.5 0 2.07z"
                                                        ></path>
                                                    </svg>
                                                </div>
                                            <% } %>
                                        </li>
                                    <% }); %>
                                </ul>
                            </div>
                        </div>
                    </div>
                    <div class="mt-6">
                        <%- include('../components/breadcrumb', {
                            breadcrumb: estate.breadcrumb
                        }) %>
                        <div class="font-semibold pt-4 pb-3 text-2xl text-encacap-main uppercase">
                            <%= estate.title %>
                        </div>
                        <div class="font-semibold">
                            <svg
                                xmlns="http://www.w3.org/2000/svg"
                                viewBox="0 0 24 24"
                                fill="none"
                                class="w-4 mr-2 -mt-1 inline"
                            >
                                <path
                                    d="M12 13.43a3.12 3.12 0 1 0 0-6.24 3.12 3.12 0 0 0 0 6.24Z"
                                    stroke="currentColor"
                                    stroke-width="1.5"
                                ></path>
                                <path
                                    d="M3.62 8.49c1.97-8.66 14.8-8.65 16.76.01 1.15 5.08-2.01 9.38-4.78 12.04a5.193 5.193 0 0 1-7.21 0c-2.76-2.66-5.92-6.97-4.77-12.05Z"
                                    stroke="currentColor"
                                    stroke-width="1.5"
                                ></path>
                            </svg>
                            <%= estate.location.long %>
                        </div>
                    </div>
                    <div
                        class="
                            mt-6 mb-4
                            border-t-2 border-b-2
                            border-gray-100
                        "
                    >
                        <div class="-my-0.5">
                            <div class="grid grid-cols-3 rid-cols-4 lg:grid-cols-5">
                                <div class="pb-4">
                                    <div class="w-12 h-0.5 mb-5"></div>
                                    <div class="text-encacap-main mb-1 text-sm font-semibold">Giá bán</div>
                                    <div class="py-1 font-semibold text-lg text-encacap-main"><%- estate.price %></div>
                                </div>
                                <div class="col-span-2 md:col-span-1 pb-4">
                                    <div class="w-12 h-0.5 mb-5"></div>
                                    <div class="text-encacap-main mb-1 text-sm font-semibold">Mã số</div>
                                    <div
                                        class="
                                            max-w-full
                                            mr-2
                                            py-1
                                            font-semibold
                                            text-lg
                                            text-encacap-main
                                            overflow-hidden
                                        "
                                    >
                                        # <%= estate.customId %>
                                    </div>
                                </div>
                            </div>
                            <div class="grid grid-cols-3 rid-cols-4 lg:grid-cols-5 border-t-2 border-gray-100">
                                <div class="pb-4">
                                    <div class="w-12 h-0.5 border-t-1 border-gray-100 mb-5"></div>
                                    <div class="text-gray-400 mb-1 text-sm font-semibold">Diện tích</div>
                                    <div class="py-1 font-semibold text-lg">
                                        <%- estate.area %>
                                    </div>
                                </div>
                                <% estateProperties.forEach(property => { %>
                                    <div class="pb-4">
                                        <div class="w-12 h-0.5 border-t-1 border-gray-100 mb-5"></div>
                                        <div class="text-gray-400 mb-1 text-sm font-semibold"><%= property.name %></div>
                                        <div class="py-1 font-semibold text-lg">
                                            <%= property.value %>
                                        </div>
                                    </div>
                                <% }) %>
                            </div>
                        </div>
                    </div>
                    <div class="flex flex-col md:hidden items-center mt-5 p-1 border-b-2 border-gray-100">
                        <div class="border-4 border-gray-200 rounded-full overflow-hidden">
                            <img
                                src="<%= contact.avatar %>"
                                alt="<%= contact.name %>"
                                class="w-20 h-20"
                                loading="lazy"
                                decoding="async"
                            />
                        </div>
                        <div class="mt-4 font-semibold"><%= contact.name %></div>
                        <div class="flex mt-5 mb-6">
                            <a
                                href="tel:<%= contact.phone %>"
                                class="
                                    flex
                                    justify-center
                                    items-center
                                    rounded-md
                                    px-3
                                    py-2
                                    font-semibold
                                    border-2 border-encacap-main
                                    text-encacap-main
                                    duration-200
                                    hover:bg-encacap-main hover:border-encacap-main hover:text-white
                                "
                            >
                                <svg
                                    xmlns="http://www.w3.org/2000/svg"
                                    viewBox="0 0 24 24"
                                    fill="currentColor"
                                    class="w-5 mr-2"
                                >
                                    <path
                                        d="M17.45 22.75c-1.13 0-2.32-.27-3.55-.79-1.2-.51-2.41-1.21-3.59-2.06-1.17-.86-2.3-1.82-3.37-2.87-1.06-1.07-2.02-2.2-2.87-3.36-.86-1.2-1.55-2.4-2.04-3.56-.52-1.24-.78-2.44-.78-3.57 0-.78.14-1.52.41-2.21.28-.71.73-1.37 1.34-1.94.77-.76 1.65-1.14 2.59-1.14.39 0 .79.09 1.13.25.39.18.72.45.96.81L10 5.58c.21.29.37.57.48.85.13.3.2.6.2.89 0 .38-.11.75-.32 1.1a4 4 0 0 1-.67.85l-.68.71c.01.03.02.05.03.07.12.21.36.57.82 1.11.49.56.95 1.07 1.41 1.54.59.58 1.08 1.04 1.54 1.42.57.48.94.72 1.16.83l-.02.05.73-.72c.31-.31.61-.54.9-.69.55-.34 1.25-.4 1.95-.11.26.11.54.26.84.47l3.32 2.36c.37.25.64.57.8.95.15.38.22.73.22 1.08 0 .48-.11.96-.32 1.41-.21.45-.47.84-.8 1.2-.57.63-1.19 1.08-1.91 1.37-.69.28-1.44.43-2.23.43Zm-11.86-20c-.55 0-1.06.24-1.55.72-.46.43-.78.9-.98 1.41-.21.52-.31 1.07-.31 1.66 0 .93.22 1.94.66 2.98.45 1.06 1.08 2.16 1.88 3.26.8 1.1 1.71 2.17 2.71 3.18 1 .99 2.08 1.91 3.19 2.72 1.08.79 2.19 1.43 3.29 1.89 1.71.73 3.31.9 4.63.35.51-.21.96-.53 1.37-.99.23-.25.41-.52.56-.84.12-.25.18-.51.18-.77 0-.16-.03-.32-.11-.5a.757.757 0 0 0-.28-.3l-3.32-2.36c-.2-.14-.38-.24-.55-.31-.22-.09-.31-.18-.65.03-.2.1-.38.25-.58.45l-.76.75c-.39.38-.99.47-1.45.3l-.27-.12c-.41-.22-.89-.56-1.42-1.01-.48-.41-1-.89-1.63-1.51-.49-.5-.98-1.03-1.49-1.62-.47-.55-.81-1.02-1.02-1.41l-.12-.3c-.06-.23-.08-.36-.08-.5 0-.36.13-.68.38-.93l.75-.78c.2-.2.35-.39.45-.56.08-.13.11-.24.11-.34 0-.08-.03-.2-.08-.32-.07-.16-.18-.34-.32-.53L6.46 3.17a.922.922 0 0 0-.37-.31c-.16-.07-.33-.11-.5-.11Zm8.36 12.26-.16.68.27-.7c-.05-.01-.09 0-.11.02ZM18.5 9.75c-.41 0-.75-.34-.75-.75 0-.36-.36-1.11-.96-1.75-.59-.63-1.24-1-1.79-1-.41 0-.75-.34-.75-.75s.34-.75.75-.75c.97 0 1.99.52 2.88 1.47.83.89 1.37 1.98 1.37 2.78 0 .41-.34.75-.75.75ZM22 9.75c-.41 0-.75-.34-.75-.75 0-3.45-2.8-6.25-6.25-6.25-.41 0-.75-.34-.75-.75s.34-.75.75-.75c4.27 0 7.75 3.48 7.75 7.75 0 .41-.34.75-.75.75Z"
                                    ></path>
                                </svg>
                                <%= contact.friendlyPhone %>
                            </a>
                            <% if (contact.zalo) { %>
                                <a
                                    href="https://www.zalo.me/<%= contact.phone %>"
                                    class="
                                        flex
                                        justify-center
                                        items-center
                                        rounded-md
                                        ml-4
                                        px-4
                                        py-2
                                        font-semibold
                                        border-2 border-encacap-zalo
                                        text-encacap-zalo
                                        bg-white
                                        duration-200
                                        hover:bg-encacap-zalo
                                        hover:text-white
                                    "
                                >
                                    Zalo
                                </a>
                            <% } %>
                        </div>
                    </div>
                    <div class="py-4 md:pt-px md:border-b-2 border-gray-100">
                        <% if (estate.description) { %>
                            <div class="font-semibold text-lg">Thông tin chi tiết</div>
                            <div class="pt-2 pb-1">
                                <%- estate.description %>
                            </div>
                        <% } %>
                        <div>
                            <div class="grid grid-cols-2 gap-4 md:gap-10 mt-4 mb-3">
                                <div
                                    class="
                                        flex
                                        justify-center
                                        items-center
                                        rounded-md
                                        border-2
                                        border-encacap-zalo
                                        bg-encacap-zalo
                                        px-4
                                        py-2
                                        text-center text-white
                                        font-semibold
                                        cursor-pointer
                                        duration-200
                                        hover:bg-white
                                        hover:text-encacap-zalo
                                    "
                                >
                                    <div class="group mr-4">
                                        <svg
                                            viewBox="144.605 378.566 159.607 63.582"
                                            xmlns="http://www.w3.org/2000/svg"
                                            class="w-7"
                                        >
                                            <path
                                                fill="currentColor"
                                                d="M 162.442 428.493 C 170.361 428.493 177.807 428.44 185.202 428.493 C 189.345 428.545 191.6 430.276 192.02 433.58 C 192.492 437.723 190.079 440.502 185.569 440.555 C 177.073 440.66 168.63 440.607 160.134 440.607 C 157.669 440.607 155.257 440.712 152.792 440.555 C 149.75 440.398 146.761 439.768 145.293 436.622 C 143.824 433.475 144.873 430.643 146.866 428.073 C 154.942 417.794 163.071 407.463 171.2 397.184 C 171.672 396.555 172.144 395.926 172.616 395.349 C 172.091 394.457 171.357 394.877 170.728 394.824 C 165.064 394.772 159.347 394.824 153.684 394.772 C 152.372 394.772 151.061 394.614 149.803 394.352 C 146.813 393.67 144.978 390.681 145.66 387.744 C 146.132 385.752 147.705 384.126 149.698 383.654 C 150.956 383.339 152.268 383.182 153.579 383.182 C 162.914 383.129 172.301 383.129 181.636 383.182 C 183.314 383.129 184.94 383.339 186.566 383.759 C 190.132 384.965 191.653 388.269 190.237 391.73 C 188.978 394.719 186.985 397.289 184.992 399.859 C 178.122 408.617 171.252 417.322 164.382 425.976 C 163.805 426.657 163.281 427.339 162.442 428.493 Z"
                                            />
                                            <path
                                                fill="currentColor"
                                                d="M 223.276 400.908 C 224.535 399.282 225.846 397.761 227.996 397.342 C 232.139 396.502 236.02 399.177 236.072 403.373 C 236.229 413.861 236.177 424.35 236.072 434.839 C 236.072 437.566 234.289 439.978 231.719 440.765 C 229.097 441.761 226.108 440.974 224.377 438.719 C 223.486 437.618 223.119 437.408 221.86 438.405 C 217.088 442.285 211.686 442.967 205.865 441.079 C 196.53 438.038 192.701 430.748 191.653 421.885 C 190.551 412.288 193.75 404.107 202.351 399.072 C 209.483 394.824 216.72 395.191 223.276 400.908 Z M 204.711 420.049 C 204.816 422.357 205.55 424.56 206.914 426.395 C 209.746 430.171 215.147 430.958 218.976 428.126 C 219.605 427.654 220.182 427.077 220.706 426.395 C 223.643 422.409 223.643 415.854 220.706 411.868 C 219.238 409.823 216.93 408.617 214.465 408.564 C 208.697 408.197 204.659 412.655 204.711 420.049 Z M 259.619 420.364 C 259.2 406.886 268.063 396.817 280.649 396.45 C 294.022 396.03 303.776 404.998 304.196 418.109 C 304.616 431.377 296.487 440.765 283.953 442.023 C 270.265 443.387 259.409 433.475 259.619 420.364 Z M 272.782 419.106 C 272.678 421.728 273.464 424.297 275.038 426.448 C 277.922 430.224 283.324 430.958 287.099 428.021 C 287.676 427.601 288.148 427.077 288.62 426.552 C 291.662 422.567 291.662 415.854 288.673 411.868 C 287.204 409.875 284.897 408.617 282.432 408.564 C 276.768 408.25 272.782 412.55 272.782 419.106 Z M 255.004 409.928 C 255.004 418.057 255.057 426.185 255.004 434.314 C 255.057 438.038 252.12 441.132 248.396 441.237 C 247.767 441.237 247.085 441.184 246.456 441.027 C 243.834 440.345 241.841 437.566 241.841 434.262 L 241.841 392.569 C 241.841 390.104 241.788 387.692 241.841 385.227 C 241.893 381.189 244.463 378.567 248.344 378.567 C 252.33 378.514 255.004 381.137 255.004 385.332 C 255.057 393.513 255.004 401.747 255.004 409.928 Z"
                                            />
                                        </svg>
                                    </div>
                                    Chia sẻ
                                </div>
                                <div
                                    class="
                                        flex
                                        justify-center
                                        items-center
                                        rounded-md
                                        border-2
                                        border-encacap-facebook
                                        bg-encacap-facebook
                                        px-4
                                        py-2
                                        text-center text-white
                                        font-semibold
                                        cursor-pointer
                                        duration-200
                                        hover:text-encacap-facebook
                                        hover:bg-white
                                    "
                                >
                                    <div class="mr-4 flex items-center text-lg">
                                        <svg
                                            xmlns="http://www.w3.org/2000/svg"
                                            viewBox="0 0 24 24"
                                            fill="none"
                                            class="w-6"
                                        >
                                            <path
                                                fill="currentColor"
                                                d="M22 16.19c0 3.64-2.17 5.81-5.81 5.81H15c-.55 0-1-.45-1-1v-5.77c0-.27.22-.5.49-.5l1.76-.03c.14-.01.26-.11.29-.25l.35-1.91a.303.303 0 00-.3-.35l-2.13.03c-.28 0-.5-.22-.51-.49l-.04-2.45c0-.16.13-.3.3-.3l2.4-.04c.17 0 .3-.13.3-.3l-.04-2.4c0-.17-.13-.3-.3-.3l-2.7.04a2.996 2.996 0 00-2.95 3.05l.05 2.75c.01.28-.21.5-.49.51l-1.2.02c-.17 0-.3.13-.3.3l.03 1.9c0 .17.13.3.3.3l1.2-.02c.28 0 .5.22.51.49l.09 5.7c.01.56-.44 1.02-1 1.02h-2.3C4.17 22 2 19.83 2 16.18V7.81C2 4.17 4.17 2 7.81 2h8.38C19.83 2 22 4.17 22 7.81v8.38z"
                                            ></path>
                                        </svg>
                                    </div>
                                    Chia sẻ
                                </div>
                            </div>
                        </div>
                    </div>
                    <% if (sameAreaEstates.totalResults) { %>
                        <div
                            class="md:hidden h-2 bg-gray-100"
                            style="width: calc(100% + 40px); margin-left: -20px; margin-right: -20px"
                        ></div>
                        <div class="pt-4 md:pb-8 border-b-0 md:border-b-2 border-gray-100">
                            <div class="font-semibold text-lg mt-1 mb-0 md:mb-6">
                                Bất động sản khác tại <%= estate.location.reservedShort %>
                            </div>
                            <%- include('../components/estatesHorizontal.ejs', {
                                gridClasses: 'grid grid-cols-1 md:gird-cols-2 lg:grid-cols-3 gap-0 md:gap-4',
                                estates: sameAreaEstates.results,
                            }) %>
                        </div>
                    <% } %>
                    <div
                        class="md:hidden h-2 bg-gray-100"
                        style="width: calc(100% + 40px); margin-left: -20px; margin-right: -20px"
                    ></div>
                    <div class="md:pt-4">
                        <div class="font-semibold text-lg mt-5 md:mt-1">Có thể bạn quan tâm</div>
                        <div class="md:mt-6">
                            <%- include('../components/estatesHorizontal.ejs', {
                                gridClasses: 'grid grid-cols-1 md:gird-cols-2 lg:grid-cols-3 gap-0 md:gap-4',
                                results: 6,
                                estates: randomEstates.results,
                            }) %>
                        </div>
                    </div>
                </div>
                <div class="hidden lg:block">
                    <form class="relative" action="/bat-dong-san-ban" method="GET">
                        <input
                            type="text"
                            name="query"
                            id="query"
                            class="
                                w-full
                                outline-none
                                rounded-md
                                py-4
                                px-5
                                ring-2 ring-gray-100
                                focus:ring-encacap-main
                            "
                            placeholder="Nhập từ khoá để tìm kiếm..."
                            value=""
                        />
                        <button
                            type="submit"
                            class="
                                absolute
                                top-3
                                right-3
                                flex
                                items-center
                                bg-encacap-main
